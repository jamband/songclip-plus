{
  "require": {
    "php": "^7.2",
    "yiisoft/yii2": "2.0.41.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "vlucas/phpdotenv": "^5.3"
  },
  "provide": {
    "bower-asset/inputmask": "*",
    "bower-asset/jquery": "*",
    "bower-asset/punycode": "*",
    "bower-asset/typeahead.js": "*",
    "bower-asset/yii2-pjax": "*"
  },
  "autoload": {
    "files": [
      "helpers/functions.php"
    ]
  },
  "scripts": {
    "dev": "composer install --quiet && npm install --silent && npm run dev",
    "dev-cli-only": "composer install --quiet",
    "clean": "rm -rf node_modules runtime/* vendor web/assets/*"
  }
}
